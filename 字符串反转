        ///字符串反转
            String o = a("dsadsa");
        System.out.println(o);

    }
    public static String a(String src){
        String result =" ";
        for(int i =src.length()-1;i>=0;i--){
                    char c =src.charAt(i);
            result = result +c;
        }
        return result;
    }
}
